<!DOCTYPE html>
<html lang="pt-br">

<head>
	<meta charset="utf-8" />
	<title>Noticias</title>
</head>

<body>

	<h1>Noticias</h1>

	<table>
		<tr>
			<td>Id</td>
			<td>Titulo</td>
			<td>Noticia</td>
		</tr>

		<% for(var i=0; i < noticias.length; i++){ %>
		<tr>
			<td><%= noticias[i].idportal_noticias %></td>
			<td><%= noticias[i].titulo %></td>
			<td><%= noticias[i].notica %></td>
		</tr>
		<% } %>
	</table>
</body>

</html>